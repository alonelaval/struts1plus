<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.WebsiteRecord" table="tb_website" >
 		<id name="websiteId" type="java.lang.Integer">
        	<column name="website_id"/>
        	<generator class="identity"/>
        </id>
        <property name="websiteName" column="website_name"/>
        <property name="domain" column="domain"/>
        <property name="websiteType" column="website_type"/>
        <property name="curCreditLevel" column="cur_credit_level"/>
        <property name="curCreditValue" column="cur_credit_value"/>
        <property name="companyId" column="company_id"/>
        <property name="websiteOwner" column="website_owner"/>
        <property name="websiteStartdate" column="website_startdate"/>
        <property name="icp" column="icp"/>
        <property name="guaranteed" column="guaranteed"/>
        <property name="deposit" column="deposit"/>
        <property name="queryTimes" column="query_times"/>
        <property name="agreedNum" column="agreed_num"/>
        <property name="opposedNum" column="opposed_num"/>
        <property name="websiteLink" column="website_link"/>
        <property name="claimStatus" column="claim_status"/>
        <property name="dataVersion" column="data_version"/>
        <property name="disabled" column="disabled"/>
        <property name="createTime" column="create_time"/>
        <property name="includedTime" column="included_time"/>
        <property name="lastupdatetime" column="lastupdatetime"/>
        <property name="creditNum" column="credit_num"/>
        <property name="websiteCategory" column="website_category"/>
        <property name="creditRank" column="credit_rank"/>

    </class>

</hibernate-mapping>